    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "branch-name-lint": "^2.1.1",
    "commitlint": "^17.4.4",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
